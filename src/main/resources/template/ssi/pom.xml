		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-war-plugin</artifactId>
				<configuration>
					<webappDirectory>${basedir}/src/main/webapp</webappDirectory>
					<warSourceDirectory>${basedir}/src/main/webapp</warSourceDirectory>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
 
	<profiles><!-- <profile> <id>windows</id> <activation> <os> Windows </os> 
			<file></file> </activation> <build> <resources></resources> <filters></filters> 
			<directory>directory</directory> </build> </profile> -->
		<profile>
			<id>product</id>
			<activation>
				<!-- <activeByDefault>true</activeByDefault> -->
				<property>
					<name>target-env</name>
					<value>prod</value>
				</property>
			</activation>
			<build>
				<resources>
					<resource>
						<directory>${basedir}/src/profiles/product/main/resources</directory>
						<excludes>
						</excludes>
					</resource>

				</resources>
				<plugins>
					<plugin>
						<artifactId>maven-antrun-plugin</artifactId>
						<executions>
							<execution>
								<id>gen-webadmin</id>
								<phase>compile</phase>
								<configuration>
									<tasks>
										<copy todir="/home/htmlfile/${project.name}">
											<fileset dir="${basedir}/src/main/html">
												<exclude name="**/.svn/" />
											</fileset>
										</copy>
										<copy todir="/home/resource/model/${project.name}">
											<fileset dir="${basedir}/src/main/webapp/WEB-INF/velocity">
												<exclude name="**/.svn/" />
											</fileset>
										</copy>
									</tasks>
								</configuration>
								<goals>
									<goal>run</goal>
								</goals>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>
		<profile>
			<id>develop</id>
			<activation>
				<property>
					<name>target-env</name>
